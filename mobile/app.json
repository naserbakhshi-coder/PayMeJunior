{
  "expo": {
    "name": "PayMeJunior",
    "slug": "paymejunior",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "scheme": "paymejunior",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#366092"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.paymejunior.app",
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "PayMeJunior needs access to your photos to select receipt images for expense processing."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#366092"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.paymejunior.app",
      "permissions": [
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-image-picker",
        {
          "photosPermission": "PayMeJunior needs access to your photos to select receipt images."
        }
      ]
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "544d3dbc-604c-4fd0-b70c-a120fdc00a34"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/544d3dbc-604c-4fd0-b70c-a120fdc00a34"
    },
    "owner": "naser420"
  }
}
